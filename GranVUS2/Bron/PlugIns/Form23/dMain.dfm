      'SELECT *,'
      
        #39#1055#1086#1095#1090'.'#1072#1076#1088'.:'#39'+NullIf(IsNull(NullIf(LTrim(postaddr),'#39#39'), LTrim(fiz' +
        'addr)),'#39#39') sec_addr,'
      #39#1058#1077#1083'.'#39'+NullIf(LTrim(o.phone),'#39#39') phoneX,'
      #39#1060#1072#1082#1089' '#39'+NullIf(LTrim(o.fax),'#39#39') faxX,'
      
        #39'('#39'+NullIf(case when o.orgname <> o.orgsname then o.orgsname els' +
        'e '#39#39' end,'#39#39')+'#39')'#39' orgsnameX,'
      '(select top 1 FIO + CHAR(13) + PHONE'
      '   from ORG_CONT'
      '  where ORGID = O.ORGID'
      '    and IS_GEN = 1) AS DIRECTOR'
      'FROM ORG O'
      
        'left JOIN KTERR ON LEFT(IsNull(O.Okato2,'#39#39'), 2)+'#39'000000000'#39' = kt' +
        'err.KODTERR'
      
        'left join ORG_CONT HEAD ON O.ORGID = HEAD.ORGID and HEAD.IS_GEN ' +
        '= 1 '
      'WHERE O.OUT_DATE IS NULL AND O.HAS_BRON = 1'
      'and LEFT(IsNull(O.Okato2,'#39#39'), 2) = convert(varchar(50),:KodTerr)')
