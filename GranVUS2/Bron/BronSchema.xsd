<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2005 sp2 U (http://www.altova.com) by Aleksey V. Meleschenin (State Unitary Enterprise of Moscow “GNPP “Granit-Center) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="MVKVBG" type="MVKVBGType" nillable="false">
		<xs:annotation>
			<xs:documentation>Корневой элемент</xs:documentation>
		</xs:annotation>
		<xs:key name="KTERRKey">
			<xs:selector xpath=".//KTERR"/>
			<xs:field xpath="KODTERR"/>
		</xs:key>
		<xs:key name="KFSKey">
			<xs:selector xpath=".//KFS"/>
			<xs:field xpath="KOD"/>
		</xs:key>
		<xs:key name="KOPFKey">
			<xs:selector xpath=".//KOPF"/>
			<xs:field xpath="KOD"/>
		</xs:key>
		<xs:key name="KOVKKey">
			<xs:selector xpath=".//KOVK"/>
			<xs:field xpath="OVK_ID"/>
		</xs:key>
		<xs:key name="ORGKey">
			<xs:selector xpath=".//ORG"/>
			<xs:field xpath="ORGID"/>
		</xs:key>
		<xs:keyref name="ORGKFSKeyRef" refer="KFSKey">
			<xs:selector xpath=".//ORG"/>
			<xs:field xpath="FS_ID"/>
		</xs:keyref>
		<xs:keyref name="ORGKOPFKeyRef" refer="KOPFKey">
			<xs:selector xpath=".//ORG"/>
			<xs:field xpath="KOPF_ID"/>
		</xs:keyref>
		<xs:keyref name="ORGKTERRKeyRef" refer="KTERRKey">
			<xs:selector xpath=".//ORG"/>
			<xs:field xpath="TERR_ID"/>
		</xs:keyref>
		<xs:keyref name="ORGKOVKKeyRef" refer="KOVKKey">
			<xs:selector xpath=".//ORG"/>
			<xs:field xpath="OVK_ID"/>
		</xs:keyref>
		<xs:unique name="ORG_CONTUnique">
			<xs:selector xpath=".//ORG_CONT"/>
			<xs:field xpath="ORGID"/>
			<xs:field xpath="IS_GEN"/>
		</xs:unique>
		<xs:keyref name="ORG_CONTKeyRef" refer="ORGKey">
			<xs:selector xpath=".//ORG_CONT"/>
			<xs:field xpath="ORGID"/>
		</xs:keyref>
		<xs:keyref name="PERKeyRef" refer="ORGKey">
			<xs:selector xpath=".//PER"/>
			<xs:field xpath="ORGID"/>
		</xs:keyref>
		<xs:keyref name="OSNKeyRef" refer="ORGKey">
			<xs:selector xpath=".//OSN"/>
			<xs:field xpath="ORGID"/>
		</xs:keyref>
		<xs:key name="SUBJKey">
			<xs:selector xpath=".//SUBJ"/>
			<xs:field xpath="SUBJ_ID"/>
		</xs:key>
		<xs:keyref name="SUBJKTERRKeyRef" refer="KTERRKey">
			<xs:selector xpath=".//SUBJ"/>
			<xs:field xpath="TERR_ID"/>
		</xs:keyref>
		<xs:unique name="SUBJ_INFOUnique">
			<xs:selector xpath=".//SUBJ_INFO"/>
			<xs:field xpath="SUBJ_ID"/>
			<xs:field xpath="SUBJ_INFO_ID"/>
		</xs:unique>
		<xs:keyref name="SUBJ_INFOKeyRef" refer="SUBJKey">
			<xs:selector xpath=".//SUBJ_INFO"/>
			<xs:field xpath="SUBJ_ID"/>
		</xs:keyref>
		<xs:unique name="SUBJ_PARAMUnique">
			<xs:selector xpath=".//SUBJ_PARAM"/>
			<xs:field xpath="SUBJ_ID"/>
			<xs:field xpath="SUBJ_PARAM_ID"/>
		</xs:unique>
		<xs:keyref name="SUBJ_PARAMKeyRef" refer="SUBJKey">
			<xs:selector xpath=".//SUBJ_PARAM"/>
			<xs:field xpath="SUBJ_ID"/>
		</xs:keyref>
		<xs:key name="FORM6HDRKey">
			<xs:selector xpath=".//FORM6HDR"/>
			<xs:field xpath="F6_ID"/>
		</xs:key>
		<xs:keyref name="FORM6ORGKeyRef" refer="FORM6HDRKey">
			<xs:selector xpath=".//ORG"/>
			<xs:field xpath="ORGID"/>
		</xs:keyref>
		<xs:keyref name="FORM6SUBJKeyRef" refer="FORM6HDRKey">
			<xs:selector xpath=".//SUBJ"/>
			<xs:field xpath="SUBJ_ID"/>
		</xs:keyref>
		<xs:unique name="FORM6Unique">
			<xs:selector xpath=".//FORM6"/>
			<xs:field xpath="F6_ID"/>
			<xs:field xpath="ROW_NO"/>
			<xs:field xpath="COL_NO"/>
		</xs:unique>
		<xs:keyref name="FORM6KeyRef" refer="FORM6HDRKey">
			<xs:selector xpath=".//FORM6"/>
			<xs:field xpath="F6_ID"/>
		</xs:keyref>
		<xs:unique name="FFSUnique">
			<xs:selector xpath=".//FFS"/>
			<xs:field xpath="SUBJ_ID"/>
			<xs:field xpath="FS_ID"/>
		</xs:unique>
		<xs:keyref name="FFSSUBJKeyRef" refer="SUBJKey">
			<xs:selector xpath=".//FFS"/>
			<xs:field xpath="SUBJ_ID"/>
		</xs:keyref>
		<xs:keyref name="FFSKFSKeyRef" refer="KFSKey">
			<xs:selector xpath=".//FFS"/>
			<xs:field xpath="FS_ID"/>
		</xs:keyref>
		<xs:unique name="FOKVEDUnique">
			<xs:selector xpath=".//FOKVED"/>
			<xs:field xpath="SUBJ_ID"/>
			<xs:field xpath="OKVED_SECTION"/>
		</xs:unique>
		<xs:keyref name="FOKVEDSUBJKeyRef" refer="SUBJKey">
			<xs:selector xpath=".//FOKVED"/>
			<xs:field xpath="SUBJ_ID"/>
		</xs:keyref>
	</xs:element>
	<xs:complexType name="MVKVBGType">
		<xs:sequence>
			<xs:element name="KTERR" type="KTERRType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Справочник военно-учетных органов (ВУО) и территорий</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KFS" type="KFSType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Справочник форм собственности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KOPF" type="KOPFType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Справочник организационно-правовых форм</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KOVK" type="KOVKType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Справочник военкоматов</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ORG" type="ORGType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Организации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ORG_CONT" type="ORG_CONTType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Руководитель, Военно-учетный работник организации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PER" type="PERType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Сведения о бронировании организации (перечни)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OSN" type="OSNType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Сведения о бронировании организации (основания)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SUBJ" type="SUBJType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Территориальные/отраслевые ВУО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SUBJ_INFO" type="SUBJ_INFOType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Органы управления ВУО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SUBJ_PARAM" type="SUBJ_PARAMType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Количественные характеристики ВУО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FORM6HDR" type="FORM6HDRType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Список агрегированных Форм № 6 ВУО и форм 6 организаций</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FORM6" type="FORM6Type" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Формы № 6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FFS" type="FFSType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Распределение организаций по видам собственности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FOKVED" type="FOKVEDType" nillable="false" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Распределение персонала подотчетных организаций по видам экономической деятельности</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="VERSION" type="str20" use="required">
			<xs:annotation>
				<xs:documentation>Версия формата предоставления данных</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="KTERRType">
		<xs:sequence>
			<xs:element name="KODTERR" type="str20" nillable="false">
				<xs:annotation>
					<xs:documentation>Код ВУО и/или территории</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NAME" type="str150" nillable="false">
				<xs:annotation>
					<xs:documentation>Наименование ВУО и/или территории</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IS_OKATO" type="xs:boolean" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак: совпадает с кодом ОКАТО (т.е. является кодом территории и, возможно, кодом соответствующего территориального ВУО)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KFSType">
		<xs:sequence>
			<xs:element name="KOD" type="kod2" nillable="false">
				<xs:annotation>
					<xs:documentation>Код формы собственности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FS_NAME" type="str100" nillable="false">
				<xs:annotation>
					<xs:documentation>Наименование формы собственности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FORFORM" type="xs:boolean" nillable="false">
				<xs:annotation>
					<xs:documentation>Признак: отдельной строкой фигурирует в документе  о распределении организаций ВУО по формам собственности </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KOPFType">
		<xs:sequence>
			<xs:element name="KOD" type="kod2" nillable="false">
				<xs:annotation>
					<xs:documentation>Код ОКОПФ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KOPF_NAME" type="str100" nillable="false">
				<xs:annotation>
					<xs:documentation>Наименование</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KOVKType">
		<xs:sequence>
			<xs:element name="OVK_ID" type="GUID" nillable="false">
				<xs:annotation>
					<xs:documentation>Идентификатор военкомата</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OVK_NAME" type="str100" nillable="false">
				<xs:annotation>
					<xs:documentation>Наименование военкомата</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OVK_ADDR" type="str250" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Адрес военкомата</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OVK_PHONE" type="str50" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Телефон военкомата</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ORGType">
		<xs:sequence>
			<xs:element name="ORGID" type="GUID" nillable="false">
				<xs:annotation>
					<xs:documentation>Идентификатор организации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ORGNAME" type="str250" nillable="false">
				<xs:annotation>
					<xs:documentation>Наименование организации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HAS_BRON" type="xs:boolean" nillable="false">
				<xs:annotation>
					<xs:documentation>Ведет бронирование</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TERR_ID" type="str20" nillable="false">
				<xs:annotation>
					<xs:documentation>Код территории</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FS_ID" type="kod2" nillable="false">
				<xs:annotation>
					<xs:documentation>Код формы собственности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="INN" type="str15" nillable="false">
				<xs:annotation>
					<xs:documentation>ИНН</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KOPF_ID" type="kod2" nillable="false">
				<xs:annotation>
					<xs:documentation>Код ОКОПФ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ORGSNAME" type="str50" nillable="false">
				<xs:annotation>
					<xs:documentation>Сокращенное наименование организации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="URADDR" type="str250" nillable="false">
				<xs:annotation>
					<xs:documentation>Юридический адрес</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MAINOKONH" type="str15" nillable="false">
				<xs:annotation>
					<xs:documentation>Основной ОКВЭД</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="F6_SHIFR" type="kod2" nillable="false">
				<xs:annotation>
					<xs:documentation>Шифр Формы № 6</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FIZADDR" type="str250" nillable="false">
				<xs:annotation>
					<xs:documentation>Фактический адрес</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PARENT" type="str100" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Вышестоящая организация</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OKPO" type="str8" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ОКПО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OKONH" type="OkonhPattern" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Список ОКВЭД через запятую</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SOOGU" type="str5" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ОКОГУ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KODREG" type="str11" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Вышестоящий ВУО курирует</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="REGDATE" type="xs:date" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата регистрации</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="REGNUM" type="str20" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Регистрационный номер</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="COMMENT" type="str500" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Примечание</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="POSTADDR" type="str250" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Почтовый адрес</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PHONE" type="str50" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Телефон</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FAX" type="str50" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Факс</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EMAIL" type="str100" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>E-mail</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WWW" type="str100" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Адрес веб-сайта</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CONFDATE" type="xs:date" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата заполнения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OKBV" type="str12" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Порядковый номер, присвоенный организации в ВУО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OCCUPATION" type="str150" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Словесное уточнение вида деятельности</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FIRST_DATE" type="xs:date" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата регистрации в ВУО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LAST_DATE" type="xs:date" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата посл. прихода отчета в ВУО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OUT_REASON" type="str60" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Основание снятия с учета организации в ВУО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OUT_DATE" type="xs:date" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата снятия с учета</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SBM_ID" type="int4" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Подчиненность
1. Федеральные органы гос.власти (ФОГВ)
2. Органы исп. власти Субъекта (ОИВС)
3. Органы местного самоуправления (ОМСУ)
4. Другие органы (ведомств.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CHK_DATE" type="xs:date" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Дата сверки в ВУО</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OKATO2" type="str50" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ОКАТО фактического адреса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OKATO3" type="str50" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ОКАТО почтового адреса</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Feature1" type="str30" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак 1 (дополнительный)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Feature2" type="str30" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак 2 (дополнительный)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Feature3" type="str30" nillable="false" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Признак 3 (дополнительный)</xs:documentation>
